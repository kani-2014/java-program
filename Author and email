import java.util.Scanner;

public class Author {
String name;
String email;
char gender;
Author(String n,String e,char g)
{
	name=n;
	email=e;
	gender=g;
}
void gender(char c)
{
	gender=c;
}
void display()
{
	System.out.println("name:"+ name  +" "+"email:"+ email+" "+"gender:"+gender+" ");
}
	public static void main(String[] args) {
	Author A=new Author("KANI","KANI123@gmailcom",'f');
	A.gender('f');
	A.display();
	
	}

}
